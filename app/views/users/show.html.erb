<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link %>
        <%'さん情報'%>
      </h2>
      <table class="table">
      <%= link_to "ログアウト", destroy_user_session_path, method: :delete, class: 'btn'%>
      <%= link_to "チャットページに戻る", :back, class: 'btn'%>
    </div> 
    <%= form_with model: current_user, local: true do |f|%>
        <tbody>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link %>
            
          </tr>
          <tr>
            <th class="table__col1">プロフィール</th>
            <td class="table__col2"><%= f.text_field :profile%>
            </td>
          </tr>
          <tr>
            <th class="table__col1">所属</th>
            <td class="table__col2"><%= f.text_field :occupation%>

            </td>
             </tr>
          <tr>
            <th class="table__col1">役職</th>
            <td class="table__col2"><%= f.text_field :position %>
            </td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        
        <%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link %>
         <%= "のプロトタイプ"%>
      </h2>
      <div class="user__card">
      </div>
      <% end %>
    </div>
  </div>
 </div>