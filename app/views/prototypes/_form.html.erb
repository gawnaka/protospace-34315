<%= form_with local: true do |f|%>
<%= link_to current_user.name, edit_user_path(current_user) %>
  <div class="field">
    <%= f.label :title, "プロトタイプの名称" %><br />
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :catch_copy, "キャッチコピー" %><br />
    <%= f.text_area :catch_copy, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :concept, "コンセプト" %><br />
    <%= f.text_area :concept, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :ActiveStorage, "プロトタイプの画像" %><br />
    <%= f.file_field :ActiveStorage %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
    <%= link_to room.name, room_messages_path(room) %>
  </div>
<% end %>